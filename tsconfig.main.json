{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
  "rootDir": "src",
  "outDir": "dist/main",
  "module": "NodeNext",
  "moduleResolution": "NodeNext",
    "lib": ["ES2022"],
    "types": ["node"],
    "noEmit": false,
    "jsx": "react-jsx"
  },
  "include": ["src/main/**/*", "src/services/**/*", "src/shared/**/*"],
  "exclude": ["src/renderer/**/*"]
}
