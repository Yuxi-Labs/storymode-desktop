{
  "compilerOptions": {
    "target": "ES2022",
  "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowJs": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "./src",
    "paths": {
      "@shared/*": ["shared/*"],
      "@services/*": ["services/*"]
    }
  }
}
